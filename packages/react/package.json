{
  "name": "@substreams/react",
  "version": "0.1.2",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && tsc --project tsconfig.build.json",
    "release": "pnpm build && ../../scripts/prerelease.sh ./package.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist/**", "!dist/**/*.tsbuildinfo", "src/**/*.ts", "!src/**/*.test.ts"],
  "dependencies": {
    "@bufbuild/connect": "^0.9.1",
    "@bufbuild/connect-web": "^0.9.1",
    "@bufbuild/protobuf": "^1.2.1",
    "@substreams/core": "^0.1.6",
    "@substreams/proxy": "^0.1.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.7",
    "@types/uuid": "^9.0.1",
    "@vitest/coverage-c8": "^0.31.2",
    "@vitest/ui": "^0.31.2",
    "react": "^18.2.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  }
}
