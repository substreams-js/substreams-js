{
  "name": "@substreams/react",
  "description": "",
  "version": "0.7.0",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/substreams-js/substreams-js.git",
    "directory": "packages/react"
  },
  "scripts": {
    "build": "pnpm build-esm && pnpm build-cjs && pnpm build-annotate && build-utils pack-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build --plugins annotate-pure-calls --out-dir build --source-maps"
  },
  "dependencies": {
    "@substreams/core": "workspace:*"
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^1.5.1",
    "@connectrpc/connect": "^1.1.4",
    "@connectrpc/connect-web": "^1.1.4",
    "@types/react": "^18.2.43",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "^1.5.1",
    "@connectrpc/connect": "^1.1.4",
    "@connectrpc/connect-web": "^1.1.4",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  }
}
