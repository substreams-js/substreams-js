{
  "name": "@substreams/react",
  "version": "0.1.10",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "release": "pnpm build && ../../scripts/prerelease.sh ./package.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist/**", "!dist/**/.tsbuildinfo", "src/**/*.ts", "!src/**/*.test.ts"],
  "repository": {
    "type": "git",
    "url": "https://github.com/substreams-js/substreams-js.git",
    "directory": "packages/react"
  },
  "dependencies": {
    "@bufbuild/connect": "^0.12.0",
    "@bufbuild/connect-web": "^0.12.0",
    "@bufbuild/protobuf": "^1.2.1",
    "@substreams/core": "^0.1.17"
  },
  "devDependencies": {
    "@types/react": "^18.2.18",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.1",
    "react": "^18.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  }
}
