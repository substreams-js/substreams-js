{
  "name": "@fubhy/substreams-proxy",
  "version": "0.0.2",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && pnpm run build:cjs && pnpm run build:esm+types",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --verbatimModuleSyntax false --outDir ./dist/cjs && echo > ./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm+types": "tsc --project tsconfig.build.json --module es2015 --outDir ./dist/esm --declaration --declarationMap --declarationDir ./dist/types && echo > ./dist/esm/package.json '{\"type\":\"module\"}' && echo > ./bin/package.json '{\"type\":\"module\"}'",
    "release": "pnpm build && ../../scripts/prerelease.sh ./package.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit && tsc --noEmit --project tsconfig.build.json"
  },
  "bin": {
    "substreams-proxy": "./bin/cli.js"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./client": {
      "types": "./dist/types/client.d.ts",
      "import": "./dist/esm/client.js",
      "default": "./dist/cjs/client.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "client": [
        "./dist/types/client.d.ts"
      ]
    }
  },
  "files": [
    "bin/**",
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts"
  ],
  "dependencies": {
    "@bufbuild/connect": "^0.8.3",
    "@bufbuild/connect-node": "^0.8.3",
    "@bufbuild/protobuf": "^1.1.0",
    "@fubhy/substreams": "^0.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.1",
    "@types/yargs": "^17.0.24",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vitest": "^0.30.1"
  }
}
