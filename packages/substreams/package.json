{
  "name": "@fubhy/substreams",
  "version": "0.0.3",
  "scripts": {
    "codegen": "rm -rf src/proto && buf generate buf.build/fubhy/substreams && buf generate buf.build/fubhy/substreams-sink-kv && buf generate buf.build/fubhy/substreams-sink-database && buf generate buf.build/fubhy/substreams-sink-entity",
    "clean": "rm -rf dist",
    "build": "pnpm run clean && pnpm run build:cjs && pnpm run build:esm+types",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --verbatimModuleSyntax false --outDir ./dist/cjs && echo > ./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm+types": "tsc --project tsconfig.build.json --module es2015 --outDir ./dist/esm --declaration --declarationMap --declarationDir ./dist/types && echo > ./dist/esm/package.json '{\"type\":\"module\"}'",
    "release": "pnpm build && ../../scripts/prerelease.sh ./package.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./sinks/kv": {
      "types": "./dist/types/sinks/kv.d.ts",
      "import": "./dist/esm/sinks/kv.js",
      "default": "./dist/cjs/sinks/kv.js"
    },
    "./sinks/database": {
      "types": "./dist/types/sinks/database.d.ts",
      "import": "./dist/esm/sinks/database.js",
      "default": "./dist/cjs/sinks/database.js"
    },
    "./sinks/entity": {
      "types": "./dist/types/sinks/entity.d.ts",
      "import": "./dist/esm/sinks/entity.js",
      "default": "./dist/cjs/sinks/entity.js"
    },
    "./proto/*.js": {
      "types": "./dist/types/proto/*.d.ts",
      "import": "./dist/esm/proto/*.js",
      "default": "./dist/cjs/proto/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "proto/*": [
        "./dist/types/proto/*.d.ts"
      ]
    }
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts"
  ],
  "dependencies": {
    "@bufbuild/connect": "^0.8.3",
    "@bufbuild/protobuf": "^1.1.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "1.18.0-1",
    "@types/node": "^20.1.4",
    "@vitest/coverage-c8": "^0.31.0",
    "@vitest/ui": "^0.31.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0"
  }
}
