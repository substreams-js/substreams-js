{
  "name": "@substreams/core",
  "version": "0.1.9",
  "license": "MIT",
  "scripts": {
    "codegen": "rm -rf src/proto && buf generate buf.build/fubhy/substreams && buf generate buf.build/fubhy/substreams-sink-kv && buf generate buf.build/fubhy/substreams-sink-database && buf generate buf.build/fubhy/substreams-sink-entity",
    "clean": "rm -rf dist",
    "build": "pnpm run clean && tsc --project tsconfig.build.json",
    "release": "pnpm build && ../../scripts/prerelease.sh ./package.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./proto": {
      "types": "./dist/proto.d.ts",
      "default": "./dist/proto.js"
    },
    "./sinks/kv": {
      "types": "./dist/sinks/kv.d.ts",
      "default": "./dist/sinks/kv.js"
    },
    "./sinks/database": {
      "types": "./dist/sinks/database.d.ts",
      "default": "./dist/sinks/database.js"
    },
    "./sinks/entity": {
      "types": "./dist/sinks/entity.d.ts",
      "default": "./dist/sinks/entity.js"
    },
    "./proto/*.js": {
      "types": "./dist/proto/*.d.ts",
      "default": "./dist/proto/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "proto/*": ["./dist/proto/*.d.ts"]
    }
  },
  "files": ["dist/**", "!dist/**/*.tsbuildinfo", "src/**/*.ts", "!src/**/*.test.ts"],
  "repository": {
    "type": "git",
    "url": "https://github.com/substreams-js/substreams-js.git",
    "directory": "packages/core"
  },
  "dependencies": {
    "@bufbuild/connect": "^0.9.1",
    "@bufbuild/protobuf": "^1.2.1"
  },
  "devDependencies": {
    "@bufbuild/buf": "1.20.0-1",
    "@substreams/testutils": "^0.0.0",
    "@types/node": "^20.2.5",
    "@vitest/coverage-c8": "^0.31.2",
    "@vitest/ui": "^0.31.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.2"
  }
}
