{
  "name": "@substreams/proxy",
  "version": "0.1.4",
  "license": "MIT",
  "scripts": {
    "codegen": "rm -rf src/proto && buf generate proto",
    "clean": "rm -rf dist",
    "start": "tsx src/cli.ts",
    "build": "pnpm run clean && tsc --project tsconfig.build.json",
    "release": "pnpm build && ../../scripts/prerelease.sh ./package.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "substreams-proxy": "./bin/cli.js"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    },
    "./proto/*.js": {
      "types": "./dist/proto/*.d.ts",
      "default": "./dist/proto/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client/index.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ],
      "proto/*": [
        "./dist/proto/*.d.ts"
      ]
    }
  },
  "files": [
    "bin/**",
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/substreams-js/substreams-js.git",
    "directory": "packages/proxy"
  },
  "dependencies": {
    "@bufbuild/connect": "^0.9.1",
    "@bufbuild/connect-node": "^0.9.1",
    "@bufbuild/protobuf": "^1.2.1",
    "@substreams/core": "^0.1.10",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@bufbuild/buf": "1.20.0-1",
    "@types/node": "^20.2.5",
    "@types/yargs": "^17.0.24",
    "@vitest/coverage-c8": "^0.31.2",
    "@vitest/ui": "^0.31.2",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.2"
  }
}
