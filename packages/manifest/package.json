{
  "name": "@substreams/manifest",
  "version": "0.0.8",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "release": "pnpm build && ../../scripts/prerelease.sh ./package.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["system.pb", "dist/**", "!dist/**/.tsbuildinfo", "src/**/*.ts", "!src/**/*.test.ts", "src/**/*.pb"],
  "repository": {
    "type": "git",
    "url": "https://github.com/substreams-js/substreams-js.git",
    "directory": "packages/manifest"
  },
  "dependencies": {
    "@bufbuild/buf": "^1.25.0",
    "@bufbuild/protobuf": "^1.2.1",
    "@effect/schema": "^0.32.0",
    "@substreams/core": "^0.1.17",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  }
}
