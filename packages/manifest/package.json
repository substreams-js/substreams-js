{
  "name": "@substreams/manifest",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "release": "pnpm build && ../../scripts/prerelease.sh ./package.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "system.pb",
    "dist/**",
    "!dist/**/.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "src/**/*.pb"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/substreams-js/substreams-js.git",
    "directory": "packages/manifest"
  },
  "dependencies": {
    "@substreams/core": "workspace:*"
  },
  "peerDependencies": {
    "@bufbuild/buf": "^1.26.1",
    "@bufbuild/protobuf": "^1.3.1",
    "@effect/schema": "^0.39.0",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.27.1",
    "@bufbuild/protobuf": "^1.3.3",
    "@effect/schema": "^0.44.0",
    "@types/node": "^20.8.7",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "yaml": "^2.3.3"
  }
}
